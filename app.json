{
  "expo": {
    "name": "Al Ansar Masjid",
    "slug": "al-ansar-masjid-app",
  "version": "1.0.10",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "alansar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true,
      "bundleIdentifier": "com.alansarmasjid.app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "alansar"
            ]
          }
        ]
      },
      "entitlements": {
        "com.apple.developer.in-app-payments": [
          "merchant.com.alansarmasjid.app"
        ]
      }
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "versionCode": 22,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/ic_launcher_foreground.png",
        "backgroundImage": "./assets/images/ic_launcher_background.png",
        "monochromeImage": "./assets/images/ic_launcher_monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.alansarmasjid.app",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "alansar"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "266a70eb-391c-4ae9-a37c-e6cbdc2067d1"
      },
      "stripePublishableKey": "pk_live_51SIKYtLloj2bckBJQ1vDAP7D8QYDPM5CO5RGIStFx4q8j4HiPu16zSFQSDnouLTdwtQePzDqQ2gIdTCWDRPyRDlX00avS46sC0"
    },
    "owner": "ibbydev"
  }
}
