{
  "name": "al-ansar-masjid-app",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "dev": "expo start",
    "clean": "node scripts/clean-build.js",
    "clean:android": "node scripts/clean-build.js",
    "clean:build:debug": "npm run clean && npm run build:debug",
    "clean:build:release": "npm run clean && npm run build:release",
    "clean:build:bundle": "npm run clean && npm run build:bundle",
    "build:debug": "cd android && gradlew assembleDebug && cd ..",
    "build:release": "cd android && gradlew assembleRelease && cd ..",
    "build:bundle": "cd android && gradlew bundleRelease && cd ..",
    "install:debug": "adb reverse tcp:8081 tcp:8081 && adb install android/app/build/outputs/apk/debug/app-debug.apk",
    "install:release": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "icons:generate:notifications": "node scripts/generate-notification-icons.js",
    "icons:generate:launcher": "node scripts/generate-launcher-icons.js",
    "eas-build-pre-build": "npm run icons:generate:notifications"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.4.1",
    "@expo/vector-icons": "^15.0.2",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-firebase/app": "^23.4.1",
    "@react-native-firebase/app-check": "^23.4.1",
    "@react-native-firebase/firestore": "^23.4.1",
    "@react-native-firebase/functions": "^23.4.1",
    "@react-native-firebase/messaging": "^23.4.1",
    "@react-native-picker/picker": "^2.11.4",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/material-top-tabs": "^7.3.8",
    "@react-navigation/native": "^7.1.8",
    "@stripe/stripe-react-native": "^0.54.1",
    "adhan": "^4.4.3",
    "expo": "~54.0.13",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-network": "~8.0.7",
    "expo-router": "~6.0.11",
